<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="SeekBarDialogPreference">
        <attr name="maximumValue" format="integer" />
        <attr name="minimumValue" format="integer" />
        <attr name="stepSize" format="integer" />
        <attr name="units" format="string" />
    </declare-styleable>
    <declare-styleable name="VolumeDialogPreference">
        <attr name="volumeType" format="string" />
        <attr name="vNoChange" format="integer" />
        <attr name="vSharedProfile" format="integer" />
        <attr name="vDisableSharedProfile" format="integer" />
    </declare-styleable>
    <declare-styleable name="BrightnessDialogPreference">
        <attr name="bNoChange" format="integer" />
        <attr name="bAutomatic" format="integer" />
        <attr name="bSharedProfile" format="integer" />
        <attr name="bDisableSharedProfile" format="integer" />
    </declare-styleable>
    <declare-styleable name="EventTypePreference">
    </declare-styleable>
    <declare-styleable name="ProfilePreference">
        <attr name="addNoActivateItem" format="integer" />
        <attr name="noActivateAsDoNotApply" format="integer" />
        <attr name="showDuration" format="integer" />
    </declare-styleable>
    <declare-styleable name="BetterNumberPickerPreference">
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="maxExternal" format="string" />
        <attr name="minExternal" format="string" />
    </declare-styleable>
    <declare-styleable name="DurationDialogPreference">
        <attr name="dMax" format="integer" />
        <attr name="dMin" format="integer" />
    </declare-styleable>
    <declare-styleable name="LocationGeofencePreference">
        <attr name="onlyEdit" format="integer" />
    </declare-styleable>
    <declare-styleable name="ApplicationsMultiSelectDialogPreference">
        <attr name="addShortcuts" format="integer" />
        <attr name="removePPApplications" format="integer" />
        <attr name="systemSettings" format="string" />
    </declare-styleable>
    <declare-styleable name="InfoDialogPreference">
        <attr name="infoText" format="string" />
    </declare-styleable>
    <declare-styleable name="ConnectToSSIDDialogPreference">
        <attr name="ctsdpDisableSharedProfile" format="integer" />
    </declare-styleable>
    <declare-styleable name="RingtonePreference">
        <attr name="ringtoneType" format="string" />
        <attr name="showSilent" format="boolean" />
        <attr name="showDefault" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PPScrimInsetsFrameLayout">
        <attr format="reference|color" name="insetForeground" />
    </declare-styleable>

    <!-- theme dependent drawables -->
    <attr name="colorAccentDisabled" format="reference" />
    <attr name="activityTextColor" format="reference" />
    <attr name="activityCommandBackgroundColor" format="reference" />
    <attr name="actionNewProfileIcon" format="reference" />
    <attr name="actionDeleteAllIcon" format="reference" />
    <attr name="actionEditProfilesIcon" format="reference" />
    <attr name="activityPopupBackground" format="reference" />
    <attr name="dragHandleIcon" format="reference" />
    <attr name="actionProfilePrefSave" format="reference" />
    <attr name="actionProfilePrefCancel" format="reference" />
    <attr name="actionProfileButtonTextStyle" format="reference" />
    <attr name="navigationDrawerBackground" format="reference" />
    <attr name="navigationDrawerText" format="reference"/>
    <attr name="navigationDrawerSubText" format="reference"/>
    <attr name="navigationDrawerSelector" format="reference"/>
    <attr name="filterStatusBarBackground" format="reference" />
    <attr name="editorDrawerSpinner" format="reference" />
    <attr name="preferenceDialogBackground" format="reference" />
    <attr name="editItemMenuIcon" format="reference" />
    <attr name="applicationSettingsLanguageIcon" format="reference" />
    <attr name="actionEventsRestartIcon" format="reference" />
    <attr name="actionEventsRestartIconInverse" format="reference" />
    <attr name="toolbarTheme" format="reference" />
